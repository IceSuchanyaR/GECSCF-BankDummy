CREATE TABLE `tbl_bank_case` (
  `bankcase_code` VARCHAR(100) NOT NULL,
  `bankcase_group` VARCHAR(50) NOT NULL,
  `bankcase_name` VARCHAR(255) NULL,
  `default_reason_code` VARCHAR(255) NULL,
  `bankcase_status`  VARCHAR(20)  NULL,
  `create_by` MEDIUMINT NOT NULL,
  `create_time` datetime NOT NULL,
  `update_by` MEDIUMINT NOT NULL,
  `update_time` DATETIME NOT NULL,
  `version` INT NOT NULL,
  PRIMARY KEY (`bankcase_code`));